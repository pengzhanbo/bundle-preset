{
  "name": "@bundle-preset/vite-basic",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@bundle-preset/shared": "workspace:*",
    "unocss": "^0.52.3",
    "unplugin-auto-import": "^0.16.2",
    "vite-plugin-mock-dev-server": "^1.1.16",
    "vite-plugin-pwa": "^0.15.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "vite": "^4.3.8"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "sourcemap": false,
    "splitting": false
  }
}
