{
  "name": "@bundle-preset/shared",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup"
  },
  "tsup": {
    "entry": [
      "./src/index.ts"
    ],
    "clean": true,
    "dts": true,
    "format": [
      "esm",
      "cjs"
    ],
    "sourcemap": false,
    "splitting": false
  },
  "dependencies": {
    "local-pkg": "^0.4.3"
  }
}
